           <span v-if=\"!$v.user.email.email\">\n              O email é inválido!\n            </span>\n          </div>\n        </b-form-group>\n        <b-form-group label=\"Telefone\" class=\"formGroup\">\n          <b-form-input\n            id=\"telefone\"\n            name=\"telefone\"\n            placeholder=\"Insira seu telefone\"\n            v-model=\"user.telefone\"\n            v-mask=\"phone_mask\"\n            type=\"text\"\n            @keypress=\"PhoneMaskValidate\"\n            @keyup.delete=\"PhoneMaskValidateDel\"\n            :class=\"{\n              'is-invalid': submitted && $v.user.telefone.$error,\n            }\"\n          ></b-form-input>\n          <div\n            v-if=\"submitted && $v.user.telefone.$error\"\n            class=\"invalid-feedback\"\n          >\n            <span v-if=\"!$v.user.telefone.required\">\n              O campo de telefone é obrigatório!\n            </span>\n            <span v-if=\"!$v.user.telefone.minLength\">\n              O telefone é inválido!\n            </span>\n          </div>\n        </b-form-group>\n      </div>\n      <hr />\n      <div class=\"dadosPagamento\"><h3 class=\"titleDados\">Pagamento</h3></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { required, email, minLength, sameAs } from \"vuelidate/lib/validators\";\nexport default {\n  data() {\n    return {\n      user: {\n        nome: \"\",\n        email: \"\",\n        telefone: \"\",\n        senha: \"\",\n        senhaConfirm: \"\",\n        statusCheck: false,\n      },\n      submitted: false,\n      phone_mask: \"(##)####-####\",\n    };\n  },\n  validations: {\n    user: {\n      nome: { required },\n      email: { required, email },\n      telefone: { required, minLength: minLength(13) },\n      senha: { required, minLength: minLength(6) },\n      senhaConfirm: { required, sameAsPassword: sameAs(\"senha\") },\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->